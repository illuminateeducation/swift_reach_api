{
	"name": "illuminate-education/swift-reach-api",
	"type": "library",
	"description": "PHP library to integrate with SwiftReach",
	"keywords": ["php", "swiftreach"],
	"homepage": "https://github.com/illuminateeducation/swift_reach_api",
	"license": "MIT",
	"require": {
		"php": ">=5.4.0",
		"guzzlehttp/guzzle": "~5.2"
	},
	"autoload": {
		"psr-0": {
			"SwiftReachApi\\Tests": "tests/",
			"SwiftReachApi": "src/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "4.*",
		"symfony/class-loader": "*",
		"satooshi/php-coveralls": "dev-master"
	}
}
